{
    "name": "laravel/laravel",
    "description": "ZombiLingo.",
    "keywords": ["GWAP", "ZombiLingo"],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0",
        "laravel/framework": "5.5.*",
        "laravelcollective/html": "~5.0",
        "predis/predis" : "~1.0",
        "symfony/finder" : "~3.2",
        "flynsarmy/csv-seeder":"1.0.*",
        "chumper/zipper": "1.0.*",
        "unisharp/laravel-filemanager": "^1.7",
        "cviebrock/laravel-elasticsearch": "^2.0",
        "illuminate/console": "~5.4",
        "barryvdh/laravel-translation-manager": "^0.4.1",
        "gwaps4nlp/faq-manager": "^1.0.0",
        "gwaps4nlp/news-manager": "^1.0.0",
        "gwaps4nlp/core": "^1.0.0"
    },
    "require-dev": {
        "symfony/dom-crawler": "3.1.*",
        "symfony/css-selector": "3.1.*",
        "fzaninotto/faker": "~1.4",
        "barryvdh/laravel-debugbar": "^2.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "phpspec/phpspec": "~2.1",
        "laravel/dusk": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database",
            "app/Models"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]        
    },
    "config": {
        "preferred-install": {
            "zombilingo/*": "source",
            "*": "dist"
        }
    }
}
